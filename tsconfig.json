{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@api/*": [
        "./api/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@images/*": [
        "./images/*"
      ],
      "@interfaces/*": [
        "./interfaces/*"
      ],
      "@models/*": [
        "./models/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@styles/*": [
        "./styles/*"
      ]
    },
    "allowJs": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_error.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}